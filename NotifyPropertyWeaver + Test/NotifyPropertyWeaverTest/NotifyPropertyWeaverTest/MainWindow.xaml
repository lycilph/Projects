<Window x:Class="NotifyPropertyWeaverTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Margin="50">
        <DockPanel>
            <Button DockPanel.Dock="Right" Content="Change" Click="Change1Click"/>
            <TextBlock Text="{Binding Count}"/>
        </DockPanel>
        
        <DockPanel>
            <Button DockPanel.Dock="Right" Content="Change" Click="Change2Click"/>
            <TextBlock Text="{Binding Text}"/>
        </DockPanel>
    </StackPanel>
</Window>
