<Window x:Class="ObservableObject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBlock Text="{Binding WrappedModel.Prop1}"/>
        <TextBlock Text="{Binding WrappedModel.Prop2}"/>
        <TextBlock Text="{Binding Total}"/>
        <Button Content="Add to Prop1" Click="AddProp1ButtonClick"/>
        <Button Content="Change model" Click="ChangeModelButtonClick"/>
    </StackPanel>
</Window>
